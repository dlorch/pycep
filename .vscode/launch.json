{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug PyCep",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/pycep/cli.py",
			"args": ["${workspaceRoot}/pycep/tests/snippets/pos/astrein.py"],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			],
			"env": {
				"PYTHONPATH": "${workspaceRoot}"
			}
		}
	]
}